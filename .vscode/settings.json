{
  "files.associations": {
    "*.h": "c",
    "*.c": "c",
    "__locale": "c",
    "__threading_support": "c",
    "vector": "c",
    "__hash_table": "c",
    "__split_buffer": "c",
    "array": "c",
    "bitset": "c",
    "initializer_list": "c",
    "string": "c",
    "string_view": "c",
    "unordered_map": "c",
    "ios": "c",
    "__bit_reference": "c",
    "utility": "c",
    "ranges": "c",
    "span": "c",
    "valarray": "c",
    "source_location": "c",
    "random": "c",
    "*.tcc": "c",
    "chrono": "c",
    "algorithm": "c",
    "fstream": "c",
    "streambuf": "c"
  },
  "makefile.makePath": "make",
  "makefile.makefilePath": "${workspaceFolder}/src/Makefile",
  "makefile.defaultBuildTarget": "w_debug",

  "C_Cpp.default.compilerPath": "x86_64-w64-mingw32-gcc.exe",
  "C_Cpp.default.cStandard": "gnu11",
  "C_Cpp.default.intelliSenseMode": "gcc-x64",

  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "C_Cpp.codeAnalysis.clangTidy.buildPath": "${workspaceFolder}/src"
}